<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Download recipe for Fujitsu DeskUpdate.</string>
    <key>Identifier</key>
    <string>com.github.autopkg-win.download.FujitsuDeskUpdate-Win</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>FujitsuDeskUpdate</string>
        <key>SEARCH_VER</key>
		<string>(?P&lt;SessionID&gt;ASPSESSIONID[A-Z]+)\t(?P&lt;CookieStr&gt;[A-Z]+)</string>
        <key>SOFTWARE_GUID</key>
		<!-- Filedownload&SoftwareGUID=5345fcd8-3f02-4199-90ad-22269d958ef8', function(data) -->
		<!-- <string>[\w]*-[\w]*-[\w]*-[\w]*-[\w]*</string> -->
		<string>"updateSetupPackage":{"id":[\w]*,"softwareID":"(?P&lt;SW_GUID&gt;[\w]*-[\w]*-[\w]*-[\w]*-[\w]*)"}</string>
		<key>USER_AGENT</key>
        <string>Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:81.0) Gecko/20100101 Firefox/81.0</string>
    </dict>
    <key>MinimumVersion</key>
    <string>1.0.5</string>
    <key>Process</key>
    <array>
<!-- <string>https://support.ts.fujitsu.com/Deskupdate/index.asp?lng=de"</string> -->
<!--         <dict>
            <key>Processor</key>
            <string>URLTextSearcher</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
				<string>https://community.chocolatey.org/packages/deskupdate#files</string>
                <key>re_pattern</key>
                <string>%SOFTWARE_GUID%</string>
            </dict>
        </dict> -->
        <dict>
            <key>Processor</key>
            <string>URLTextSearcher</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
				<string>https://webdownloads2.ts.fujitsu.com/deskupdate_5_1/data/api/v1/update/5.01.0064.0</string>
                <key>re_pattern</key>
                <string>%SOFTWARE_GUID%</string>
				<key>curl_opts</key>
				<array>
					<string>--request</string>
					<string>GET</string>
				</array>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
			<string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>https://support.ts.fujitsu.com/Download/Download.asp</string>
                <key>request_headers</key>
                <dict>
                    <key>user-agent</key>
                    <string>%USER_AGENT%</string>
                </dict>
				<key>curl_opts</key>
				<array>
					<string>--data-raw</string>
					<!-- <string>SoftwareGUID=5345FCD8-3F02-4199-90AD-22269D958EF8</string> -->
					<string>SoftwareGUID=%SW_GUID%</string>
					<string>-c</string>
					<string>%RECIPE_CACHE_DIR%\cookie.txt</string>
				</array>
			    <key>filename</key>
                <string>%NAME%.exe</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>WindowsSignatureVerifier</string>
            <key>Arguments</key>
            <dict>
                <key>input_path</key>
                <string>%pathname%</string>
                <key>expected_subject</key>
                <string>CN=FUJITSU CLIENT COMPUTING LIMITED, O=FUJITSU CLIENT COMPUTING LIMITED, STREET="1-1-2, KASHIMADA, SAIWAI-KU", L=KAWASAKI, S=Kanagawa, PostalCode=212-0058, C=JP</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>StopProcessingIf</string>
            <key>Arguments</key>
            <dict>
                <key>predicate</key>
                <string>download_changed == False</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>
