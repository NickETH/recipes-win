<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the latest version of iTunes x64 MSI for Windows.</string>
    <key>Identifier</key>
    <string>com.github.NickETH.download.iTunesMSI-Win</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>iTunes</string>
        <!--Downloads 64 bit version by default. Uncomment following win32 url or override to get 32bit version--> 
        <!--<key>DOWNLOAD_URL</key>
        <string>https://www.apple.com/itunes/download/win32</string>-->
        <key>DOWNLOAD_URL</key>
        <string>https://www.apple.com/itunes/download/win64</string>
        <key>USER_AGENT</key>
        <string>Mozilla/5.0 (Windows NT 6.1; WOW64; Trident/7.0; AS; rv:11.0) like Gecko</string>
        <key>SEARCH_URL</key>
        <string>https://www.apple.com/itunes/download/win64</string>
        <key>SEARCH_PATTERN</key>
        <string>itunes.exe</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.9</string>
    <key>Process</key>
    <array>
<!--         <dict>
            <key>Processor</key>
            <string>URLTextSearcher</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%SEARCH_URL%</string>
                <key>re_pattern</key>
                <string>%SEARCH_PATTERN%</string>
            </dict>
        </dict> -->
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%DOWNLOAD_URL%</string>
                <key>request_headers</key>
                <dict>
                        <key>user-agent</key>
                        <string>%USER_AGENT%</string>
                </dict>
                <key>filename</key>
                <string>%NAME%.exe</string>
            </dict>
        </dict>
<!--         <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>https://download.videolan.org/vlc/%version%/%PLATFORM%/vlc-%version%-%PLATFORM%.msi</string>
                <key>filename</key>
                <string>vlc-%version%-%PLATFORM%.msi</string>
            </dict>
        </dict> -->
        <dict>
            <key>Processor</key>
            <string>WindowsSignatureVerifier</string>
            <key>Arguments</key>
            <dict>
                <key>input_path</key>
                <string>%pathname%</string>
                <key>expected_subject</key>
                <string>CN=Apple Inc., O=Apple Inc., L=Cupertino, S=California, C=US</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>
